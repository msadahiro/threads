@{
    ViewData["Title"] = "Customers";
}
<div id="CustomersPage">
    <div class="container">
            <table>
                <thead>
                    <tr>
                        <th>Customer Name</th>
                        <th>Created Date</th>
                        <th>Actions</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        @{
                            foreach(var user in @ViewBag.ListOfUsers){
                                <td>@user.Name</td>
                                <td>@Convert.ToDateTime(user.CreatedAt).ToString("MMMM dd, yyyy")</td>

                            }
                        }
                        
                    </tr>
                </tbody>
            </table>
    </div>
</div>
